    <hudson.tasks.Shell>
      <command>IMAGEN_DOCKER_PEDIDOSOGA=pedidosga
ARCHIVO_YML_PEDIDOSGA=pedidos-deploy.yaml
VERSION_PEDIDOSGA=$(sudo docker images | awk &apos;($1 == &quot;pocga/pedidosga&quot;) {print $2 += .1; exit}&apos;)

cd build/docker/pedidosga 
sudo docker login -u $DOCKER_USER -p $DOCKER_PASS
sudo docker build -t $DOCKER_USER/$IMAGEN_DOCKER_PEDIDOSOGA:$VERSION_PEDIDOSGA . 
sudo docker push $DOCKER_USER/$IMAGEN_DOCKER_PEDIDOSOGA:$VERSION_PEDIDOSGA 
sudo docker logout

curl  https://raw.githubusercontent.com/pocga/IaCGA/master/microservices/$ARCHIVO_YML_PEDIDOSGA --output $ARCHIVO_YML_PEDIDOSGA

sed -i &apos;s/IMAGETAG/&apos;&quot;$VERSION_PEDIDOSGA&quot;&apos;/g&apos; $ARCHIVO_YML_PEDIDOSGA

export PATH=$HOME/bin:$PATH
/snap/bin/kubectl apply -f $ARCHIVO_YML_PEDIDOSGA
</command>
    </hudson.tasks.Shell>
